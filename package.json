{
  "name": "draft-ssr",
  "version": "0.0.0",
  "scripts": {
    "spa:start:dev": "ng run draft-ssr:serve:development --host 0.0.0.0",
    "spa:start:prod": "ng run draft-ssr:serve:production",
    "spa:build:dev": "ng run draft-ssr:build:development",
    "spa:build:prod": "ng run draft-ssr:build:production",
    "spa:test": "ng test",
    "ssr:start:dev": "ng run draft-ssr:serve-ssr:development",
    "ssr:start:prod": "ng run draft-ssr:serve-ssr:production",
    "ssr:build:dev": "ng run draft-ssr:build:development && ng run draft-ssr:server:development",
    "ssr:build:prod": "ng run draft-ssr:build:production && ng run draft-ssr:server:production",
    "ssr:prerender:dev": "ng run draft-ssr:prerender:development",
    "ssr:prerender:prod": "ng run draft-ssr:prerender:production",
    "spite:build": "svg2sprite ./src/assets/icons/bootstrap ./src/assets/icons/sprite.svg --stripAttrs class --stripAttrs fill",
    "node:start": "node dist/draft-ssr/server/main.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@angular/service-worker": "~12.2.0",
    "@fingerprintjs/fingerprintjs": "^3.3.2",
    "@nguniversal/express-engine": "^12.1.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "dayjs": "^1.10.6",
    "dompurify": "^2.3.1",
    "express": "^4.15.2",
    "incremental-dom": "^0.7.0",
    "jdenticon": "^3.1.1",
    "markdown-it": "^12.2.0",
    "markdown-it-incremental-dom": "^2.1.0",
    "markdown-it-link-attributes": "^3.0.0",
    "prismjs": "^1.24.1",
    "rxjs": "~6.6.0",
    "split-grid": "^1.0.11",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.3",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "~12.2.0",
    "@nguniversal/builders": "^12.1.0",
    "@types/compression": "^1.7.1",
    "@types/dompurify": "^2.2.3",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/markdown-it": "^12.2.1",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^12.11.1",
    "@types/prismjs": "^1.16.6",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "prettier": "2.3.2",
    "svg2sprite-cli": "^2.0.1",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.3.5"
  }
}
