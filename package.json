{
  "name": "draft-ssr",
  "version": "0.0.1",
  "description": "Node frontend project",
  "author": "Denis Iakimenko",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start:dev": "ng run draft-ssr:serve:development",
    "start:dev:network": "ng run draft-ssr:serve:development --host 0.0.0.0 --disable-host-check",
    "start:deploy": "firebase use draft-ssr && firebase deploy --only functions,hosting",
    "start:lint": "ng lint",
    "build:ssr:prod": "ng run draft-ssr:build:production",
    "build:spite": "svg2sprite ./src/assets/icons/bootstrap ./src/assets/icons/sprite.svg --inline --stripAttrs class --stripAttrs fill && node sprite && prettier --write src/index.html --log-level silent",
    "build:sitemap": "node sitemap.js",
    "dist:start": "node dist/draft-ssr/server/server.mjs",
    "dist:analyze": "webpack-bundle-analyzer dist/draft-ssr/stats.json"
  },
  "dependencies": {
    "@angular/animations": "^17.0.5",
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/fire": "^17.0.0",
    "@angular/forms": "^17.0.5",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "@angular/platform-server": "^17.0.5",
    "@angular/router": "^17.0.5",
    "@angular/service-worker": "^17.0.5",
    "@angular/ssr": "^17.0.5",
    "@floating-ui/dom": "^1.5.3",
    "autosize": "^6.0.1",
    "colorjs.io": "^0.4.5",
    "daisyui": "^4.4.2",
    "dayjs": "^1.11.7",
    "dompurify": "^3.0.0",
    "emoji-mart": "^5.5.2",
    "express": "^4.18.2",
    "firebase": "^10.6.0",
    "jdenticon": "^3.2.0",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-collapsible": "^2.0.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-multimd-table": "^4.2.0",
    "markdown-it-plain-text": "^0.3.0",
    "markdown-it-smartarrows": "^1.0.1",
    "markdown-it-tasks": "^0.0.5",
    "markdown-it-video": "^0.6.3",
    "mime": "^4.0.1",
    "morphdom": "^2.7.0",
    "ngx-image-cropper": "^7.1.2",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "qrcode": "^1.5.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.4.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1700.5",
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@tailwindcss/typography": "^0.5.9",
    "@types/compression": "^1.7.2",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.17",
    "@types/markdown-it": "^13.0.6",
