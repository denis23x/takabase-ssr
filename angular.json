{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "draft-ssr": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "prismjs",
              "dompurify",
              "qrcode",
              "markdown-it-attrs",
              "markdown-it-bracketed-spans",
              "markdown-it-collapsible",
              "markdown-it-emoji",
              "markdown-it-link-attributes",
              "markdown-it-mark",
              "markdown-it-multimd-table",
              "markdown-it-plain-text",
              "markdown-it-smartarrows",
              "markdown-it-tasks",
              "markdown-it-video"
            ],
            "outputPath": "dist/draft-ssr/browser",
            "index": "src/index.html",
            "main": "src/main.browser.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest",
              "src/robots.txt",
              "src/sitemap.xml"
            ],
            "styles": [
              "src/styles.scss",
              {
                "input": "node_modules/prismjs/themes/prism.min.css",
                "bundleName": "prism-default",
                "inject": false
              },
              {
                "input": "node_modules/prismjs/themes/prism-coy.min.css",
                "bundleName": "prism-coy",
                "inject": false
              },
              {
                "input": "node_modules/prism-themes/themes/prism-darcula.css",
                "bundleName": "prism-darcula",
                "inject": false
              },
              {
                "input": "node_modules/prism-themes/themes/prism-holi-theme.css",
                "bundleName": "prism-holi-theme",
                "inject": false
              },
              {
                "input": "node_modules/prism-themes/themes/prism-night-owl.css",
                "bundleName": "prism-night-owl",
                "inject": false
              },
              {
                "input": "node_modules/prismjs/themes/prism-okaidia.min.css",
                "bundleName": "prism-okaidia",
                "inject": false
              },
              {
                "input": "node_modules/prism-themes/themes/prism-one-dark.css",
                "bundleName": "prism-one-dark",
                "inject": false
              },
              {
                "input": "node_modules/prism-themes/themes/prism-one-light.css",
                "bundleName": "prism-one-light",
                "inject": false
              },
              {
                "input": "node_modules/prismjs/themes/prism-solarizedlight.min.css",
                "bundleName": "prism-solarizedlight",
                "inject": false
              },
              {
                "input": "node_modules/prism-themes/themes/prism-vs.css",
                "bundleName": "prism-vs",
                "inject": false
              },
              {
                "input": "node_modules/prism-themes/themes/prism-synthwave84.css",
                "bundleName": "prism-synthwave84",
                "inject": false
              },
              {
                "input": "node_modules/prismjs/themes/prism-tomorrow.min.css",
                "bundleName": "prism-tomorrow",
                "inject": false
              },
              {
                "input": "node_modules/prismjs/themes/prism-twilight.min.css",
                "bundleName": "prism-twilight",
                "inject": false
              }
            ],
            "scripts": [
              {
                "input": "node_modules/emoji-mart/dist/browser.js",
                "inject": true,
                "bundleName": "emoji-mart"
              },
              {
                "input": "node_modules/autosize/dist/autosize.js",
                "inject": true,
                "bundleName": "autosize"
              },
              {
                "input": "ngsw.js",
                "inject": false
              }
            ],
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "8kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "statsJson": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "draft-ssr:build:production"
            },
            "development": {
              "browserTarget": "draft-ssr:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "draft-ssr:build"
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/draft-ssr/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "sourceMap": true,
              "extractLicenses": true,
              "namedChunks": false,
              "buildOptimizer": true
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false
            }
          },
          "defaultConfiguration": "production"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routesFile": "prerender.txt",
            "guessRoutes": false
          },
          "configurations": {
            "production": {
              "browserTarget": "draft-ssr:build:production",
              "serverTarget": "draft-ssr:server:production"
            },
            "development": {
              "browserTarget": "draft-ssr:build:development",
              "serverTarget": "draft-ssr:server:development"
            }
          },
          "defaultConfiguration": "production"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "version": 2,
            "browserTarget": "draft-ssr:build:production",
            "serverTarget": "draft-ssr:server:production",
            "prerenderTarget": "draft-ssr:prerender:production"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
