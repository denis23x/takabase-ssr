{
  "semi": false,
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "insertPragma": true
      }
    },
    {
      "files": "*.html",
      "options": {
        "singleQuote": false
      }
    }
  ]
}
