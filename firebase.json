        "bread-compressor -a brotli,gzip '.firebase/takabase-prod/hosting/**/*.{js,css}'",
        "html-minifier-terser --input-dir .firebase/takabase-prod/functions/dist/takabase-ssr/browser --output-dir .firebase/takabase-prod/functions/dist/takabase-ssr/browser --file-ext html --preserve-line-breaks --remove-comments --minify-js",
        "html-minifier-terser --input-dir .firebase/takabase-prod/hosting --output-dir .firebase/takabase-prod/hosting --file-ext html --preserve-line-breaks --remove-comments --minify-js"
      ],
      "target": "takabase-prod",
      "source": ".",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "frameworksBackend": {
